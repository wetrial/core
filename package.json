{
  "name": "@wetrial/core",
  "version": "1.1.2",
  "description": "react component library",
  "keywords": [
    "antd components",
    "react components"
  ],
  "main": "./lib/index.js",
  "module": "./es/index.js",
  "types": "./lib/index.d.ts",
  "authors": {
    "name": "xiexingen",
    "email": "1002275364@qq.com"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.com/"
  },
  "repository": "https://github.com/wetrial/core",
  "homepage": "https://github.com/wetrial/core",
  "scripts": {
    "start": "npm run dev",
    "dev": "cross-env LAN=zh-CN father doc dev",
    "dev:en-us": "father doc dev",
    "js": "node ./.ts2js.js",
    "build": "node ./scripts/clean-old-build.js && father build && tsc --declarationDir ./es --noImplicitAny false && node ./scripts/build-wind-up.js",
    "test": "father test",
    "cov": "father test --coverage",
    "help": "father help",
    "precommit": "father pre-commit",
    "build:doc": "father doc build &&cross-env LAN=zh-CN father doc build",
    "publish:build": "node ./scripts/clean-old-build.js && father build && tsc --declarationDir ./es --noImplicitAny false && node ./scripts/build-wind-up.js && cp -r ./es/* . && rm -rf ./es",
    "pub:doc": "now --prod",
    "patch": "yarn version --patch && git push origin master && git push origin --tags",
    "minor": "yarn version --minor && git push origin master && git push origin --tags",
    "major": "yarn version --major && git push origin master && git push origin --tags",
    "lint": "yarn lint:js && yarn lint:style && yarn lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && yarn lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./",
    "lint:prettier": "check-prettier lint",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "prettier": "prettier -c --write \"**/*\""
  },
  "files": [
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "dependencies": {
    "axios": "^0.19.2",
    "dva-model-extend": "^0.1.2",
    "store": "^2.0.12"
  },
  "peerDependencies": {
    "@ant-design/icons": "*",
    "antd": "*",
    "dva": "*",
    "path-to-regexp": "*",
    "react": "*",
    "react-dom": "*",
    "redux": "*"
  },
  "devDependencies": {
    "@ant-design/icons": "^4.0.0-alpha.19",
    "@types/jest": "^24.0.15",
    "@umijs/fabric": "^2.0.2",
    "antd": "^4.0.0-rc.4",
    "babel-plugin-import": "^1.12.0",
    "check-prettier": "^1.0.3",
    "cross-env": "^6.0.3",
    "del": "^5.1.0",
    "docz-theme-umi-hooks": "^0.0.7",
    "dva": "2.6.0-beta.16",
    "enzyme": "^3.10.0",
    "father": "^2.13.3",
    "husky": "^3.1.0",
    "jackbox": "^0.2.0",
    "lint-staged": "^9.5.0",
    "now": "^16.7.0",
    "path-to-regexp": "^6.1.0",
    "prettier": "^1.19.1",
    "raw-loader": "^3.1.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "redux": "^4.0.5",
    "sylvanas": "^0.4.2",
    "typescript": "^3.7.5",
    "umi": "^2.13.3"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "checkFiles": [
    "src/**/*.js*",
    "src/**/*.ts*"
  ],
  "license": "MIT"
}
